{
  "name": "hopeshelter-design",
  "version": "1.0.0",
  "description": "* Portfolio Home Page\r * React+Redux\r * Bootstrap + SASS \r * RWD (Responsive Web Design)\r * Pinterest Like",
  "main": "index.js",
  "engines": {
    "node": ">=10.0"
  },
  "scripts": {
    "build:dll": "webpack --config=webpack/webpack.config.dll.js",
    "build": "cross-env NODE_ENV=production webpack --config=webpack/webpack.config.prod.js",
    "start": "node dist/server.generated.js",
    "dev": "concurrently --kill-others -p \"[{name}]\" -n \"Client,Server,StartServer\" -c \"bgBlue.bold,bgGreen.bold,bgBlack.bold,black.bgWhite.bold\" \"npm run dev:client\" \"npm run dev:server\" \"npm run dev:start\" ",
    "dev:client": "cross-env BROWSER=true node ./src/server/server.dev.js",
    "dev:server": "cross-env NODE_ENV=development webpack --watch --config=./webpack/webpack.config.dev.server.js",
    "dev:start": "nodemon dev/server.generated.dev.js",
    "api": "nodemon apiserver/main.js",
    "stat": "webpack --progress --profile --json --config=webpack/webpack.config.dev.js > stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grace951/grace951.github.io.git"
